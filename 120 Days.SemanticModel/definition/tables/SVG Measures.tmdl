	measure 'Complete Card Visual' = ```
			
			
			var _salesacheivedvalue = [Sales Acheived]
			var _salesacheived = FORMAT(_salesacheivedvalue, "0.0%")
			var _targetvalue = SELECTEDVALUE('Target Table'[Value]) / 100
			var _target = FORMAT(_targetvalue,"0.0%")
			var _togovalue = _targetvalue - _salesacheivedvalue
			var _togo = FORMAT(_togovalue, "0.0%")
			var _acheivedpctvalue = DIVIDE(_salesacheivedvalue,_targetvalue)
			var _acheivedpct = FORMAT(_acheivedpctvalue,"0%")
			var _progressbarflow = _salesacheivedvalue * 372
			var _progressbartargetline = ( _targetvalue* 372 ) + 24
			
			var _svg = 
			"data:image/svg+xml;utf8,
			<svg width='420' height='220' viewBox='0 0 420 220' xmlns='http://www.w3.org/2000/svg'>
			
			  <defs>
			    <linearGradient id='bgGrad' x1='0' y1='0' x2='1' y2='1'>
			      <stop offset='0%' stop-color='#1c1b3a'/>
			      <stop offset='100%' stop-color='#3b1c63'/>
			    </linearGradient>
			
			    <filter id='glow'>
			      <feGaussianBlur stdDeviation='4' result='coloredBlur'/>
			      <feMerge>
			        <feMergeNode in='coloredBlur'/>
			        <feMergeNode in='SourceGraphic'/>
			      </feMerge>
			    </filter>
			  </defs>
			
			  <!-- Background -->
			  <rect width='420' height='220' rx='16' fill='url(#bgGrad)'/>
			
			  <!-- Title -->
			  <text x='24' y='36' fill='#cfcdf7' font-size='14' font-family='Segoe UI, Arial'>
			    Sales Acheived
			  </text>
			
			  <!-- Achieved pill -->
			  <rect x='270' y='18' rx='12' ry='12' width='120' height='26' fill='#6d4aff'/>
			  <text x='330' y='36' text-anchor='middle' fill='#ffffff'
			        font-size='12' font-family='Segoe UI, Arial'>
			    Achieved: "& _acheivedpct &"
			  </text>
			
			  <!-- Main Value -->
			  <text x='24' y='90' fill='#ffffff' font-size='40'
			        font-weight='700' font-family='Segoe UI, Arial'>
			    "& _salesacheived &"
			  </text>
			
			  <!-- Target -->
			  <text x='24' y='115' fill='#bdbbe8' font-size='12'
			        font-family='Segoe UI, Arial'>
			    Target: "& _target &" | To Go: "& _togo &"
			  </text>
			
			  <!-- Progress Bar Background -->
			  <rect x='24' y='140' width='372' height='10' rx='5' fill='#3a3966'/>
			
			  <!-- Progress Bar Fill (60%) -->
			  <rect x='24' y='140' width='"& _progressbarflow &"' height='10' rx='5'
			        fill='#ff77ff' filter='url(#glow)'/>
			
			  <!-- ðŸŽ¯ Target Marker at 90% -->
			  <line x1='"& _progressbartargetline &"' y1='134' x2='"& _progressbartargetline &"' y2='156'
			        stroke='#ffffff' stroke-width='2' opacity='0.9'/>
			
			  <!-- Optional target label -->
			  <text x='"& _progressbartargetline &"' y='130' text-anchor='middle'
			        fill='#ffffff' font-size='10'
			        font-family='Segoe UI, Arial'>
			    "& _target &"
			  </text>
			
			  <!-- Progress Label -->
			  <text x='210' y='168' text-anchor='middle'
			        fill='#cfcdf7' font-size='12'
			        font-family='Segoe UI, Arial'>
			    "& _acheivedpct &" of Target
			  </text>
			
			</svg>
			"
			RETURN
			_svg
			```
